<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".categoriaCelulares">

    <LinearLayout
        android:id="@+id/linearLayout9"
        android:layout_width="382dp"
        android:layout_height="578dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView35"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <ImageButton
                android:id="@+id/btnCategoriaCelularMotoE40"
                android:layout_width="140dp"
                android:layout_height="134dp"
                app:srcCompat="@drawable/_539149_1" />

            <TextView
                android:id="@+id/textView36"
                android:layout_width="match_parent"
                android:layout_height="40dp" />

            <ImageButton
                android:id="@+id/btnCategoriaCelularMotoG20"
                android:layout_width="match_parent"
                android:layout_height="146dp"
                app:srcCompat="@drawable/_6466071_160_160" />

            <TextView
                android:id="@+id/textView34"
                android:layout_width="match_parent"
                android:layout_height="42dp" />

            <ImageButton
                android:id="@+id/btnCategoriaCelularGalaxyA32"
                android:layout_width="match_parent"
                android:layout_height="136dp"
                app:srcCompat="@drawable/galaxya32" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="227dp"
            android:layout_height="559dp"
            android:orientation="vertical">

            <TextView
                android:id="@+id/txtCategoriaBicicletaEspecificacaoBicicletaAluminio"
                android:layout_width="213dp"
                android:layout_height="40dp" />

            <TextView
                android:id="@+id/txtCategoriaCelularEspecificacaoMotorolaE40"
                android:layout_width="match_parent"
                android:layout_height="78dp"
                android:text="Smartphone Motorola Moto E40 64GB Grafite 4G Tela 6.5 Pol. Câmera Tripla 48MP Selfie 8MP Android 11.0" />

            <TextView
                android:id="@+id/txtCategoriaCelularPrecoMotorolaE40"
                android:layout_width="wrap_content"
                android:layout_height="96dp"
                android:text="R$ 990,00" />

            <TextView
                android:id="@+id/txtCategoriaCelularEspecificacaoMotorolaG20"
                android:layout_width="216dp"
                android:layout_height="42dp"
                android:text="Smartphone Motorola Moto G20 128GB 4gb Ram - Verde" />

            <TextView
                android:id="@+id/txtCategoriaCelularPrecoMotorolaG20"
                android:layout_width="wrap_content"
                android:layout_height="81dp"
                android:text="R$ 1350,00" />

            <TextView
                android:id="@+id/txtCategoriaBicicletaEspecificacaoBicicletaKsw"
                android:layout_width="125dp"
                android:layout_height="45dp" />

            <TextView
                android:id="@+id/txtCategoriaCelularEspecificacaoGalaxyA32"
                android:layout_width="216dp"
                android:layout_height="80dp"
                android:text="Smartphone Samsung Galaxy A32 128GB 4G Wi-Fi Tela 6.4'' Dual Chip 4GB RAM Câmera Quádrupla + Selfie 20MP - Azul" />

            <TextView
                android:id="@+id/txtCategoriaCelularPrecoGalaxyA32"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="R$ 1200,00" />

        </LinearLayout>
    </LinearLayout>

    <TextView
        android:id="@+id/textView45"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Celulares"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.04" />

    <LinearLayout
        android:id="@+id/linearLayout5"
        android:layout_width="411dp"
        android:layout_height="61dp"
        android:background="#A1BFFF"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.997">

        <ImageButton
            android:id="@+id/btnHome"
            android:layout_width="67dp"
            android:layout_height="match_parent"
            android:backgroundTint="#A1BFFF"
            app:srcCompat="@drawable/ic_baseline_home_24"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/botaoLista"
            android:layout_width="64dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:backgroundTint="#A1BFFF"
            app:srcCompat="@drawable/ic_baseline_list_24"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/botaoCarrinho"
            android:layout_width="70dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:backgroundTint="#A1BFFF"
            app:srcCompat="@drawable/ic_baseline_shopping_cart_24"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/botaoVendedor"
            android:layout_width="60dp"
            android:layout_height="match_parent"

            android:layout_weight="1"
            android:backgroundTint="#A1BFFF"
            app:srcCompat="@drawable/ic_baseline_shopping_bag_24"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/botaoPefil"
            android:layout_width="61dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:backgroundTint="#A1BFFF"
            app:srcCompat="@drawable/ic_baseline_account_circle_24"
            tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>