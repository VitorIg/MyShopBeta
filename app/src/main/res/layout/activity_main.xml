            <TextView
                android:id="@+id/txtHomePrecoProduto2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="R$ 120,00" />
        </LinearLayout>

    </LinearLayout>

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="75dp"
        android:layout_height="56dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/whatsapp_image_2022_03_22_at_18_28_18" />

    <ImageButton
        android:id="@+id/btn_Figura_B"
        android:layout_width="57dp"
        android:layout_height="67dp"
        android:backgroundTint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.231"
        app:srcCompat="@drawable/car_ui_icon_chevron" />

    <TextView
        android:id="@+id/txv_rotulos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
