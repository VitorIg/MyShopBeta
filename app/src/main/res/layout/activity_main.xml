            <TextView
                android:id="@+id/txtHomeNomeProduto1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Galaxy A32" />

            <TextView
                android:id="@+id/txtHomePrecoProduto1"
                android:layout_width="71dp"
